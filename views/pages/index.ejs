<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/head', {title: locals.settings ? locals.settings.settings.description : ''}) %>
</head>

<body>

  <!-- ======= Header ======= -->
  <header id="header" class="fixed-top  header-transparent ">
    <%- include('../partials/header') %>
  </header><!-- End Header -->

  <!-- ======= Hero Section ======= -->
  <section id="hero" class="d-flex align-items-center">

    <div class="container">
      <div class="row">
        <div
          class="col-lg-6 d-lg-flex flex-lg-column justify-content-center align-items-stretch pt-5 pt-lg-0 order-2 order-lg-1"
          data-aos="fade-up">
          <div>
            <%- locals.settings ? locals.settings.settings.homeArea : 'Home Area?' %> 
            <a href="#" class="download-btn"><i class="bx bxl-play-store"></i>Google Play</a>
            <a href="#" class="download-btn"><i class="bx bxl-apple"></i>App Store</a>
          </div>
        </div>
        <div class="col-lg-6 d-lg-flex flex-lg-column align-items-stretch order-1 order-lg-2 hero-img"
          data-aos="fade-up">
          <img src="assets/img/hero-img.png" class="img-fluid" alt="">
        </div>
      </div>
    </div>

  </section><!-- End Hero -->

  <main id="main">
    <!-- ======= Pricing Section ======= -->
    <section id="pricing" class="pricing">
      <div class="container">
        <div class="section-title">
          <h2>Pricing</h2>
        </div>
        <div class="row">
          <% if (locals.categories) { %>
          <% categories.forEach(category => { %>
          <div class="col-lg-4">
            <div class="col-md-12 box" data-aos="fade-right">
              <div class="icon">
                <img src="<%= category.image ? category.image.replace('public/','/') : '' %>" alt="" srcset="">
              </div>
              <h3><%= category.title %></h3>
              <div class="d-flex justify-content-between">
                <div class="col-md-6">
                  <h4><%= category.count %> <span>Stock</span></h4>
                </div>
                <div class="vr"></div>

                <div class="col-md-6">
                  <h4><%= category.price %> â‚º<span>Price</span></h4>
                </div>
              </div>
              <ul>
                <li><i class="bx bx-check"></i><%= category.description %></li>
              </ul>
              <a href="/category/<%= category.slug %>" class="get-started-btn">Buy</a>
            </div>
          </div>
          <% }) %>
          <% } %>
        </div>
      </div>
    </section><!-- End Pricing Section -->


    <!-- ======= Details Section ======= -->
    <section id="details" class="details">
      <div class="container">

        <div class="row content">
          <div class="col-md-4" data-aos="fade-right">
            <img src="assets/img/details-1.png" class="img-fluid" alt="">
          </div>
          <div class="col-md-8 pt-4 check" data-aos="fade-up">
            <%- locals.settings ? locals.settings.settings.detailsArea : 'Home Area?' %> 
          </div>
        </div>


      </div>
    </section><!-- End Details Section -->

